msgid "Disabled"
msgstr "გამორთულია"
msgid "Enabled"
msgstr "ჩართულია"
msgid "Advanced options"
msgstr "დამატებითი ოფციები"
msgid "Database type"
msgstr "მონაცემთა ბაზის ტიპი"
msgid "Error"
msgstr "შეცდომა"
msgid "Site name"
msgstr "საიტის სახელი"
msgid "Drupal"
msgstr "Drupal"
msgid "E-mail address"
msgstr "ელფოსტის მისამართი"
