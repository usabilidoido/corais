msgstr "!name полето е задължително."
msgid "Protected"
msgstr "Защитен"
msgid "Processing"
msgstr "Обработва"
msgid "Finished"
msgstr "Приключен"
msgid "Default time zone"
msgstr "Часова зона по подразбиране"
