/**
 * Implementation of hook_enable().
 */
function ahah_helper_demo_enable() {
  drupal_set_message(t("The AHAH helper demo module has been enabled. Give a
                       look at the <a href=\"!demo-form-url\">demo form</a>
                       and don't forget to disable this module!",
                       array('!demo-form-url' => url('ahah_helper_demo'))));
}
